#include <iostream>
#include <vector>

using namespace std;
int findDayDigit(string day){

	if (day == "Mon")
		return 1;
	if (day == "Tue")
		return 2;
	if (day == "Wed")
		return 3;
	if (day == "Thu")
		return 4;
	if (day == "Fri")
		return 5;
	if (day == "Sat")
		return 6;
	if (day == "Sun")
		return 7;
	return -1;

}
void outputDays(pair<int, string> pair) {
	int date = pair.first;
	string start = pair.second;
	int st = findDayDigit(start);

	vector<int> *month = new vector<int>();
	for (int i = 1; i <= date; i++) {
		month->push_back(st);
		st++;
		if (st==8)
			st=1;
	}

	for(auto *it=month->begin();it!=month->end();++it){
		cout<<*it<<" "<<flush;
	}
	delete month;
}

int main() {
	int n;
	vector<pair<int, string> > *inp = new vector<pair<int, string> >;
	cin >> n;
	for (int i = 0; i < n; i++) {
		string start;
		int date;
		cin >> date >> start;
		inp->push_back(make_pair(date, start));
	}
	vector<pair<int, string> >::iterator it;
	for (it = inp->begin(); it != inp->end(); ++it) {
		outputDays(*it);
	}

	delete[] inp;
	return 0;
}
