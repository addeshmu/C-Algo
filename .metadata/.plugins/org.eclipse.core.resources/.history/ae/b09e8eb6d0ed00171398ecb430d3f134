#include <iostream>
#include <vector>
#include <string>
#include <sstream>
using namespace std;

int main() {
	int n;
	vector< pair<int,string> > *inp = new vector< pair<int,string> >;
	cin>>n;
	for (int i= 0;i<n;i++) {
		string s;
		cin>>s;
		stringstream ss(s);
		string token;
		while(getline(ss,token,' ')){
			cout<< token;
		}
	}
	vector<int> *dayCount  = new vector<int>();
	for (int i=0;i<7;i++) {
		dayCount->push_back(0);
	}

	return 0;
}
