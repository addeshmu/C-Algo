#include <iostream>
#include <vector>

using namespace std;

int main() {
	int n;
	vector< pair<int,string> > *inp = new vector< pair<int,string> >;
	cin>>n;
	for (int i= 0;i<n;i++) {
		string start;
		int date;
		cin>>date>>start;
		inp->push_back(make_pair(date,start));
	}
	vector<int> *dayCount  = new vector<int>();
	for (int i=0;i<7;i++) {
		dayCount->push_back(0);
	}
	delete dayCount;
	delete [] inp;
	return 0;
}
