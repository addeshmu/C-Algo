#include <iostream>
#include "Initiate.h"
using namespace std;

int main() {
	string s;
	int *i;
	int m  = 10;
	i = &m;
	cout<<i;
	cin>>s;
	Initiate init(s);
	init.showVal();
	return 0;
}
